    "jsonfile": "^2.4.0",
    "jsonschema": "^1.1.1",
    "prop-types": "^15.5.6",
    "ramda": "^0.23.0",
    "react": "^15.5.3",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-test-utils": "^15.5.1",
